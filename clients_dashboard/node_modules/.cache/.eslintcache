         </tr>\n                {tasks.map((task, index) => {\n                  if (valueFromUpperCard === task.project_name) {\n                    return (\n                      <tr key={index}>\n                        <td>{task.task}</td>\n                        <td>{task.project_name}</td>\n                        <td>{task.completed_status}</td>\n                        <td>{task.start_date.slice(0, 11)}</td>\n                        <td>{task.end_date.slice(0, 11)}</td>\n                        <td>\n                          <input\n                            type=\"radio\"\n                            id=\"edit\"\n                            name=\"fav_language\"\n                            value=\"HTML\"\n                          ></input>\n                        </td>\n                        <td>\n                          <input\n                            type=\"radio\"\n                            id=\"delete\"\n                            name=\"fav_language\"\n                            value=\"HTML\"\n                          ></input>\n                        </td>\n                      </tr>\n                    );\n                  }\n                  return null;\n                })}\n              </tbody>\n            </table>\n          </div>\n        </div>\n      )}\n    </div>\n  );\n}\n\nexport default Tasks;\n","/Users/stephanierowe/clients_dashboard/clients_dashboard/src/components/Uppercard.jsx",[],[],"/Users/stephanierowe/clients_dashboard/clients_dashboard/src/components/Allcards.jsx",["98","99"],[],"/Users/stephanierowe/clients_dashboard/clients_dashboard/src/components/Lowercard.jsx",[],[],"/Users/stephanierowe/clients_dashboard/clients_dashboard/src/components/Schedule.jsx",[],[],"/Users/stephanierowe/clients_dashboard/clients_dashboard/src/components/Progress.jsx",["100"],[],"/Users/stephanierowe/clients_dashboard/clients_dashboard/src/components/Graph.jsx",["101","102"],[],"/Users/stephanierowe/clients_dashboard/clients_dashboard/src/components/UpperCard.jsx",["103"],[],"import React, { useState } from \"react\";\nimport Contacts from './Contacts';\nimport Projects from './Projects';\n\nfunction UpperCard({onChange}) {\n  const [valueA, setValueAState] = useState(\"\");\n\n  function handleValueAChange(newValue) {\n    setValueAState(newValue);\n    onChange(newValue)\n  }\n\n  return (\n    <>\n    <Contacts />\n    <Projects onChange={handleValueAChange}/>\n    </>\n  );\n}\n\nexport default UpperCard;","/Users/stephanierowe/clients_dashboard/clients_dashboard/src/components/LowerCard.jsx",["104"],[],"import React, { useState } from \"react\";\nimport Tasks from \"./Tasks\";\nimport Progress from \"./Progress\";\n\nfunction LowerCard({valueFromUpperCard,valueFromLowerCard, onChange}) {\n  const [valueB, setValueBState] = useState(\"\");\n\n  function handleValueBChange(newValue) {\n    setValueBState(newValue);\n    onChange(newValue)\n  }\n\n  return (\n    <>\n    <div className=\"lowerCard\">\n      <Tasks \n      valueFromUpperCard={valueFromUpperCard}\n      onChange={handleValueBChange} />\n      <Progress valueFromUpperCard={valueFromUpperCard}\n      onChange={handleValueBChange}\n      />\n      </div>\n    </>\n  );\n}\n\nexport default LowerCard;\n","/Users/stephanierowe/clients_dashboard/clients_dashboard/src/hooks/useFetchTasks.js",["105"],[],"/Users/stephanierowe/clients_dashboard/clients_dashboard/src/hooks/useFetchProjects.js",["106"],[],"// useFetchProject.js\nimport React,{ useState, useEffect } from \"react\";\n\nfunction useFetchProjects() {\n  const [projects, setProjects] = useState([]);\n  const [error, setError] = useState(null);\n\n\n  useEffect(() => {\n    const projectsApi = \"http://127.0.0.1:8090/api/collections/projects/records/\";\n\n    fetch(projectsApi)\n    .then(response => {\n      if(!response.ok){\n        throw new Error('failed to fetch data');\n      }\n      return response.json();\n    })\n    .then(data => {\n      setProjects(data.items);\n    })\n      .catch(error => {\n        setError(error.message);\n    })\n  }, []);\n  return { projects, error };\n}\n\nexport default useFetchProjects;\n","/Users/stephanierowe/clients_dashboard/clients_dashboard/src/hooks/useForm.js",["107","108"],[],"// useTaskForm\nimport React, { useState } from \"react\";\n\nfunction useForm() {\n  const [state, setState] = useState({\n    showForm: false,\n    formValues: {\n      projectName: \"\",\n      task: \"\",\n      startDate: \"\",\n      endDate: \"\",\n    }\n  });\n\n  const { showForm, formValues } = state;\n\n  function openForm() {\n    setState((prevState) => ({\n      ...prevState,\n      showForm: true,\n    }));\n  }\n\n  function closeForm() {\n    setState((prevState) => ({\n      ...prevState,\n      showForm: false,\n    }));\n  }\n\n  function handleSubmit(event) {\n    event.preventDefault();\n\n    const { projectName, task, startDate, endDate, completed_status } = state.formValues;\n    const data = {\n      project_name: projectName,\n      task: task,\n      start_date: startDate,\n      end_date: endDate,\n      completed_status: \"Open\"\n    };\n  \n    fetch('http://127.0.0.1:8090/api/collections/tasks/records/', {\n      method: 'POST',\n      headers: {\n        'Content-Type': 'application/json'\n      },\n      body: JSON.stringify(data)\n    })\n      .then(response => response.json())\n      .then(responseData => {\n        console.log('API response:', responseData);\n      })\n      .catch(error => {\n        console.error('Error:', error);\n      });\n  \n    // Reset the form values and update state\n    setState((prevState) => ({\n      ...prevState,\n      showForm: false,\n      formValues: {\n        projectName: \"\",\n        task: \"\",\n        startDate: \"\",\n        endDate: \"\"\n      }\n    }));\n  }\n\n  function handleInputChange(event) {\n    const { name, value } = event.target;\n    setState((prevState) => ({\n      ...prevState,\n      formValues: {\n        ...prevState.formValues,\n        [name]: value,\n      },\n    }));\n  }\n\n  return {state, setState, openForm, showForm, formValues, closeForm, handleSubmit, handleInputChange};\n}\n\nexport default useForm;\n","/Users/stephanierowe/clients_dashboard/clients_dashboard/src/hooks/myContext.js",[],[],{"ruleId":"109","severity":1,"message":"110","line":36,"column":15,"nodeType":"111","endLine":36,"endColumn":39},{"ruleId":"112","severity":1,"message":"113","line":2,"column":17,"nodeType":"114","messageId":"115","endLine":2,"endColumn":25},{"ruleId":"112","severity":1,"message":"116","line":2,"column":27,"nodeType":"114","messageId":"115","endLine":2,"endColumn":36},{"ruleId":"112","severity":1,"message":"117","line":2,"column":38,"nodeType":"114","messageId":"115","endLine":2,"endColumn":48},{"ruleId":"118","severity":1,"message":"119","line":37,"column":19,"nodeType":"111","endLine":37,"endColumn":76},{"ruleId":"112","severity":1,"message":"120","line":4,"column":8,"nodeType":"114","messageId":"115","endLine":4,"endColumn":16},{"ruleId":"118","severity":1,"message":"119","line":26,"column":19,"nodeType":"111","endLine":26,"endColumn":83},{"ruleId":"112","severity":1,"message":"121","line":7,"column":18,"nodeType":"114","messageId":"115","endLine":7,"endColumn":23},{"ruleId":"112","severity":1,"message":"121","line":9,"column":18,"nodeType":"114","messageId":"115","endLine":9,"endColumn":23},{"ruleId":"112","severity":1,"message":"122","line":44,"column":27,"nodeType":"114","messageId":"115","endLine":44,"endColumn":29},{"ruleId":"112","severity":1,"message":"123","line":6,"column":10,"nodeType":"114","messageId":"115","endLine":6,"endColumn":16},{"ruleId":"112","severity":1,"message":"124","line":6,"column":10,"nodeType":"114","messageId":"115","endLine":6,"endColumn":16},{"ruleId":"112","severity":1,"message":"125","line":2,"column":7,"nodeType":"114","messageId":"115","endLine":2,"endColumn":12},{"ruleId":"112","severity":1,"message":"126","line":2,"co